spring.application.name=rss_spring

mybatis.mapper-locations=classpath:mappers/*xml
mybatis.type-aliases-package=com.example.rss_spring.mybatis.entity

server.port = 8087

spring.mvc.pathmatch.matching-strategy = ant_path_matcher

swagger.enabled = true


spring.datasource.url = jdbc:mysql://localhost:3306/rea?useUnicode=true&characterEncoding=UTF-8&serverTimezone=UTC&useSSL=false
spring.datasource.username = your_user_name

spring.datasource.password = your_password
spring.datasource.driver-class-name = com.mysql.cj.jdbc.Driver
